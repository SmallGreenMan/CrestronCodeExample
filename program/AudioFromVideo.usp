Integer_Function CheckRealConnectionFree(integer out, integer in, integer zone)
{
	integer i, i1, i2, cmd;

	switch(zone)
	{
    	case (1):
		{i1 = 1; i2 = 5;}
		case (2):
		{i1 = 6; i2 = 7;}
		case (3):
		{i1 = 8; i2 = 10;}
	}
       
	cmd = 0;
	i = i1;
	While (i < i2+1)
	{
		if (video_FB[AudioChanelNum[i]] = in)
		{
			cmd = i + 100;
			i = i2;	
		}

		i = i + 1;
	}
    if (cmd = 0)
	{
		i = i1;
		While (i < i2+1)
		{
			if (video_FB[AudioChanelNum[i]] = 0)
			{
				cmd = i;
				i = i2;	
			}
			i = i + 1;
		}
	}


	return (cmd);
}
Integer_Function CheckRealConnectionYes(integer out, integer in, integer zone)
{
	integer i, i1, i2, cmd;

	switch(zone)
	{
    	case (1):
		{i1 = 1; i2 = 5;}
		case (2):
		{i1 = 6; i2 = 7;}
		case (3):
		{i1 = 8; i2 = 10;}
	}
       
	cmd = 0;
	i = i1;
	While (i < i2+1)
	{
		if (video_FB[AudioChanelNum[i]] = in)
		{
			cmd = i + 10;
			i = i2;	
		}

		i = i + 1;
	}

	return (cmd);
}


Function CheckVideoOut(integer out)
{
	integer zone;
	integer cmd;
     
	zone = ZoneId[out];
	if (zone>0)
	{
		cmd = CheckRealConnectionYes(out,video_FB[out],zone);
		if (cmd>0)
			AudioFromVideo_out[out] = 1;
		else
			AudioFromVideo_out[out] = 0;  
    		
	}
}

Function CheckFreeChanels()
{
	integer i, y, chengeFlag, CpId;
	integer n, zone, ActiveFlag[10], AudioFromVideo_Flag[256];
         
	n = 0;
	for(zone = 1 to 3)
	{
		y = 1;
			While(VideoInZone[zone][y] > 0)
			{
				AudioFromVideo_Flag[VideoInZone[zone][y]] = 0;
              	//AudioFromVideo_out[VideoInZone[zone][y]] = 0;  	
				y = y + 1;
			}
	}
	zone = 1;
	for	(i = 1 to 5)
		if (video_FB[AudioChanelNum[i]]=0)
		{
			n = n + 1;
			ShowControl_text[i] = "";
		}
		else
		{
			ShowControl_text[i] =  InputName[video_FB[AudioChanelNum[i]]];
			y = 1;
			While(VideoInZone[zone][y] > 0)
			{
            	if (video_FB[AudioChanelNum[i]] = video_FB[VideoInZone[zone][y]])
				{
					ActiveFlag[i] = 1;
					AudioFromVideo_Flag[VideoInZone[zone][y]] = 1;
              		//AudioFromVideo_out[VideoInZone[zone][y]] = 1;  	
				}
				y = y + 1;
			}
		}

	
	debug[1] = ">>>> Zone 1 have " + itoa(n) + " free Chanels";
	NumFreeAudioChInZone[1] = itoa(n);	
	ZoneFreeChanels[1] = n;	

	n = 0;
	zone = 2;
	for	(i = 6 to 7)
		if (video_FB[AudioChanelNum[i]]=0)
		{
			n = n + 1;
			ShowControl_text[i] = "";
		}
		else
		{
			ShowControl_text[i] =  InputName[video_FB[AudioChanelNum[i]]];
			y = 1;
			While(VideoInZone[zone][y] > 0)
			{
            	if (video_FB[AudioChanelNum[i]] = video_FB[VideoInZone[zone][y]])
				{
					ActiveFlag[i] = 1;
					AudioFromVideo_Flag[VideoInZone[zone][y]] = 1;
              		//AudioFromVideo_out[VideoInZone[zone][y]] = 1;   	
				}
				y = y + 1;
			}
		}

	debug[1] = ">>>> Zone 2 have " + itoa(n) + " free Chanels";
	NumFreeAudioChInZone[2] = itoa(n);
	ZoneFreeChanels[2] = n;

	n = 0;
	zone = 3;
	for	(i = 8 to 10)
		if (video_FB[AudioChanelNum[i]]=0)
		{
			n = n + 1;
			ShowControl_text[i] = "";

		}
		else
		{
			ShowControl_text[i] =  InputName[video_FB[AudioChanelNum[i]]];
			y = 1;
			While(VideoInZone[zone][y] > 0)
			{
				debug[1] = ">>>> Zone 3 have FbInDeembeder = " + itoa(video_FB[AudioChanelNum[i]]) + " out = "+itoa(VideoInZone[zone][y])+" FbInZoneOut = "+itoa(video_FB[VideoInZone[zone][y]]);
            	if (video_FB[AudioChanelNum[i]] = video_FB[VideoInZone[zone][y]])
				{
					ActiveFlag[i] = 1;
					AudioFromVideo_Flag[VideoInZone[zone][y]] = 1;
              		//AudioFromVideo_out[VideoInZone[zone][y]] = 1;   	
				}
				y = y + 1;
			}
		}

	debug[1] = ">>>> Zone 3 have " + itoa(n) + " free Chanels";
	NumFreeAudioChInZone[3] = itoa(n);
	ZoneFreeChanels[3] = n;

      
	for(zone = 1 to 3)
	{
		y = 1;
			While(VideoInZone[zone][y] > 0)
			{
              	AudioFromVideo_out[VideoInZone[zone][y]] = AudioFromVideo_Flag[VideoInZone[zone][y]];  	
				y = y + 1;
			}
	}

	chengeFlag = 0;
	for(i = 1 to 10)
	{
		if (ActiveFlag[i] = 0)
		{
			ShowAudioControl[i] = 0;
	    	if (video_FB[AudioChanelNum[i]]>0)
			{
				switch(i)
				{
                	Case (1):
					{
						Zone = 1;
					}
                	Case (2):
					{
						Zone = 1;
					}
                	Case (3):
					{
						Zone = 1;
					}
                	Case (4):
					{
						Zone = 1;
					}
                	Case (5):
					{
						Zone = 1;
					}
              		Case (6):
					{
						Zone = 2;
					}
                	Case (7):
					{
						Zone = 2;
					}
                	Case (8):
					{
						Zone = 3;
					}  
                	Case (9):
					{
						Zone = 3;
					}
                	Case (10):
					{
						Zone = 3;
					}

				}

				chengeFlag = 1;	
	
					video_Control[AudioChanelNum[i]] = 0;			// --- Раскомутация
		 			
					CpId = (zone - 1)* 25 + i + 11;
	
					ShowControl_text[i] = "";
					CP_off[CpId] = 1;
					delay(1);
					CP_off[CpId] = 0;
			}
		}
		else
			ShowAudioControl[i] = 1;
	}
}
    
Function StartWait()
{
	Wait(110, WaitRechecAudio)
	{
    	CheckFreeChanels();
	}		
}
  
/*
	// --- Колличество микрофонов в зоне
	ZoneNumMic[1] = 0;
	ZoneNumMic[2] = 3;
	ZoneNumMic[3] = 4;
	ZoneNumMic[4] = 3;
	ZoneNumMic[5] = 2;

	// --- Микрофоны в зоне начинаются с 
	ZoneNumMicStart[1] = 0;
	ZoneNumMicStart[2] = 5;
	ZoneNumMicStart[3] = 8;
	ZoneNumMicStart[4] = 3;
	ZoneNumMicStart[5] = 1;
*/ 
Function ConectMicToVcs(integer zone, integer vcsN[])	// --- Коммутируем группу микрофонов в кодеки вкс
{
	integer i, y, CpId;
	
	if (ZoneNumMic[zone] > 0)
	{
       	for (i = 1 to 4)	// --- кодек
		{
			for (y = 1 to ZoneNumMic[zone]) // --- микрофоны
			{
				CpId = (i + 2)* 25 + ZoneNumMicStart[zone] + y - 1;
            	if (vcsN[i] > 0)	// --- коммутируем или нет
				{
					CP_on[CpId] = 1;	
					delay(1);
					CP_on[CpId] = 0;
					delay(1);
				} 
				else
				{
					CP_off[CpId] = 1;	
					delay(1);
					CP_off[CpId] = 0;
					delay(1);
				}   	
			}  	
		}  	
	}	
}
     
Function CheckMicToVSC()
{
	integer i, y,  i1, i2, vcsN[4], zone;
       
	for(zone = 1 to 5)
	{	       
		vcsN[1] = 0;
		vcsN[2] = 0;
		vcsN[3] = 0;
		vcsN[4] = 0;
		i = 1;
		While (VideoInZone[zone][i]>0)
		{
			for (y = 1 to 4)
			{
				//VideoInZone
				if (video_FB[VideoInZone[zone][i]] = VcsInInNum[y])
				{
					vcsN[y] = 1;
					y = 5;	
				}
			}
			i = i + 1;
		}
		ConectMicToVcs(zone, vcsN);
	}
}

Function StartWaitMicToVSC()
{
	Wait(120, WaitRechecMicToVSC)
	{
    	CheckMicToVSC();
	}		
}



