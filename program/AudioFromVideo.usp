PUSH letsDoIt
{
	CheckFreeChanels();
}

Push MicAuto
{
	if (MicAutoFlag = 1)
	{
		MicAuto_fb = 0;
        MicAutoFlag = 2;
	}
	else 
	{
		MicAuto_fb = 1;
        MicAutoFlag = 1;

		CancelWait(WaitRechecMicToVSC);
		StartWaitMicToVSC();
	}		
}  
Push AudioAuto
{
	if (AudioAutoFlag = 1)
	{
		AudioAuto_fb = 0;
        AudioAutoFlag = 2;
	}
	else 
	{
		AudioAuto_fb = 1;
        AudioAutoFlag = 1;

		CancelWait(WaitRechecAudio);
		StartWait();
	}		
}                      


Push AudioFromVideo_LetsDoIt
{
	integer out;
	integer zone;
	integer CpId;
	integer in;
	integer FreeCh;
	integer i;

	out = GetLastModifiedArrayIndex ();
	in = video_FB[out];   
	zone = ZoneId[out];
	debug[1] = ">>>> VideoOut is " + itoa(out) + " VideoIn Is "+ itoa(in)+" zone is " + itoa(zone);
	if (zone>0)
	{
		debug[1] = ">>>> Start CheckRealConnection  " + itoa(FreeCh);
		FreeCh = CheckRealConnectionFree(out,in,zone);	
		debug[1] = ">>>> FreeCh is " + itoa(FreeCh);
        
		if (FreeCh > 0)							
		{
			if (FreeCh < 100)
			{
				video_Control[AudioChanelNum[FreeCh]] = in;			// --- Коммутация на свободный деэмбедер
	 	
				CpId = (zone - 1)* 25 + FreeCh + 11;
				
				ShowControl_text[FreeCh] = InputName[in];
				CP_on[CpId] = 1;	
				delay(5);
				CP_on[CpId] = 0;		
			}
			else
			{
				FreeCh = FreeCh - 100;
				video_Control[AudioChanelNum[FreeCh]] = 0;			// --- Раскомутация
	 	
				CpId = (zone - 1)* 25 + FreeCh + 11;

				ShowControl_text[FreeCh] = "";
				CP_off[CpId] = 1;
				delay(5);
				CP_off[CpId] = 0;
			}
                   
			Delay(10);
			//i = 1;
			//While(VideoInZone[zone][i]>0)
			//{
           	//	CheckVideoOut(VideoInZone[zone][i]); 		
			//	i = i + 1;
			//}
                     
			if (AudioAutoFlag = 1)
				CheckFreeChanels();	// --- Проверяем наличие свободных каналов и прописываем текст имена деимбедеров
		}		
	}
}

CHANGE video_FB
{
	integer but, zone, in, out, FreeCh, CpId;
	but = GetLastModifiedArrayIndex();
	//CheckVideoOut(but);
                                 
	if (AudioAutoFlag = 1)
	{
		CancelWait(WaitRechecAudio);
		StartWait();
			
		if ((but = 39)||(but = 40))
		{
			switch(but)
			{
            	Case (39):
				{
					FreeCh = 6;
				}
            	Case (40):
				{
					FreeCh = 7;
				}

			}
                   
			zone = 2;
			in = video_FB[but];
        	if (in>0)
			{
				video_Control[AudioChanelNum[FreeCh]] = in;			// --- Коммутация на свободный деэмбедер
	 	
				CpId = (zone - 1)* 25 + FreeCh + 11;
				
				ShowControl_text[FreeCh] = InputName[in];
				CP_on[CpId] = 1;	
				delay(5);
				CP_on[CpId] = 0;  	
			}
			Else
			{
				video_Control[AudioChanelNum[FreeCh]] = 0;			// --- Раскомутация
	 	
				CpId = (zone - 1)* 25 + FreeCh + 11;

				ShowControl_text[FreeCh] = "";
				CP_off[CpId] = 1;
				delay(5);
				CP_off[CpId] = 0;
			}
		}
	}

	if (MicAutoFlag = 1)
	{
		CancelWait(WaitRechecMicToVSC);
		StartWaitMicToVSC();
	}

}
 




